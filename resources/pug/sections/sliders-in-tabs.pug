mixin sliders-in-tabs(data)
	if data.popular
		section.section.section_sliders-in-tabs#sliders-in-tabs
			.container
				.row
					.col-12.col-lg-10
						.section__nav
							.scrollbar-inner
								ul.list.list_unstyled.nav.nav-tabs.section__nav-list.row.no-gutters.flex-nowrap(role="tablist")
									li.col-auto.list__item
										a.link.section__title.active.js-product-tab#popular-tab(href="#popular-products" data-toggle="tab" role="tab" aria-controls="popular-products" aria-selected="true")
											h2 Популярные товары
									li.col-auto.list__item
										a.link.section__title.js-product-tab#viewed-tab(href="#viewed-products" data-toggle="tab" role="tab" aria-controls="viewed-products" aria-selected="false")
											h2 Вы смотрели
				.tab-content
					.tab-pane.fade.show.active#popular-products(role="tabpanel" aria-labelledby="popular-tab")
						.swiper-container.js-product-slider
							.swiper-wrapper
								.swiper-slide
									.product-card(tabindex="0")
										.product-card__content
											.product-card__header
												a.product-card__img.product-card__img_full(href="javascript: void(0);")
													img(src="images/content/products/product-img-11.jpg" alt="Изображение продукта")
												a.product-card__brand(href="javascript: void(0);" aria-label="Перейти к бренду Roca")
													img(src="images/content/brands/brand-img-1.png" alt="Roca")
												ul.list.list_unstyled.product-card__marks.marks-list
													li.list__item.list__item_num.bg_red 25%
											.product-card__body
												a.product-card__title(href="javascript: void(0);") Мебель для ванны Albina Bamboo стандарт, угловая, белая
												.product-card__price
													span.old 8 345
													span.cost.cost_new 3 567
													span.currency лей / шт
											.product-card__footer
												.product-card__info
													.product-card__availability.product-card__availability_true В наличии
													.product-card__code Артикул: 90858
												.product-card__controls
													button.btn.product-card__btn.product-card__btn_favorite.js-to-favorite(type="button" aria-label="В избранное")
														<svg class="btn__icon btn__icon_default" role="img" aria-hidden="true" width="24" height="24"><use xlink:href="#svg-icon-heart"></use></svg>
														<svg class="btn__icon btn__icon_active" role="img" aria-hidden="true" width="24" height="24"><use xlink:href="#svg-icon-heart-fill"></use></svg>
													button.btn.product-card__btn.product-card__btn_cart.active.js-to-cart(type="button" aria-label="Добавлено в корзину. Отменить добавление")
														<svg class="btn__icon btn__icon_default" role="img" aria-hidden="true" width="24" height="24"><use xlink:href="#svg-icon-shop"></use></svg>
														<svg class="btn__icon btn__icon_active" role="img" aria-hidden="true" width="24" height="24"><use xlink:href="#svg-icon-shop-added"></use></svg>
								.swiper-slide
									.product-card(tabindex="0")
