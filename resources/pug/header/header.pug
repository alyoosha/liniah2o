				ul.list.list_unstyled.header__top-nav.row.align-items-center
					li.col-auto.list__item
						a.link(href="javascript: void(0);")
							span.link__icon(aria-hidden="true")
								<svg role="img" aria-hidden="true" width="18" height="18"><use xlink:href="#svg-icon-delivery"></use></svg>
							span.link__text Оплата и доставка
					li.col-auto.list__item
